    "schema/*.json",
    "node_modules/leaflet/dist/**/*.{css,png}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://odin.si.c-s.fr/plugins/git/ia3d-demo/eodag-labextension.git"

{
  "name": "eodag-labextension",
  "version": "0.1.0",
  "description": "Searching remote sensed imagery from various image providers",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://odin.si.c-s.fr/plugins/git/ia3d-demo/eodag-labextension",
  "bugs": {
    "url": "https://odin.si.c-s.fr/plugins/git/ia3d-demo/eodag-labextension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "CS-SI",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "node_modules/leaflet/dist/**/*.{css,png}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://odin.si.c-s.fr/plugins/git/ia3d-demo/eodag-labextension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "@jupyterlab/cells": "^0.19.1",
    "@jupyterlab/notebook": "^0.19.2",
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@types/leaflet-draw": "^0.4.13",
    "@types/lodash": "4.14.120",
    "@types/react-dom": "~16.0.7",
    "@types/react-leaflet": "^1.1.6",
    "@types/react-virtualized": "^9.18.12",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.4.0",
    "leaflet-draw": "^1.0.4",
    "lodash": "^4.17.11",
    "prop-types": "^15.5.2",
    "react": "^16.4.2",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.4.2",
    "react-leaflet": "^2.2.0",
    "react-leaflet-draw": "^0.19.0",
    "react-select": "^2.3.0",
    "react-virtualized": "^9.21.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "resolutions": {
    "**/@types/react": "~16.4.13",
    "**/@types/react-dom": "~16.0.7"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
